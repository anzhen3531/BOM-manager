        <action name="batchPrintGlobal" type="wvs"/>
        <action name="hotList" type="user"/>
        <action name="myARExperiences" type="arpublish"/>
        <action name="separator" type="separator"/>
        <action name="softwareDownloads" type="netmarkets"/>
        <action name="startSysLog" type="extLog"/>
        <action name="stopSysLog" type="extLog"/>
        <action name="separator" type="separator"/>
        <action name="emailPage" type="team"/>
        <action name="copyPage" type="object"/>
        <submodel name="open"/>
        <action name="separator" type="separator"/>
        <action name="signOut" type="fba"/>
        <action name="wexman" type="com.wincomplm.wex.manager" wexid="com.wincomplm.wex.manager"/>
    </model>
</actionmodels>