<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE listofactions SYSTEM "actions.dtd">
<listofactions>

    <include href="config/actions/ext/extChangeManagement-actions.xml" />
    <include href="config/actions/ext/extFolderManagement-actions.xml" />
    <objecttype name="document" class="wt.doc.WTDocument"
                resourceBundle="com.ptc.windchill.enterprise.doc.documentResource">
        <!--   创建WTDocument 自定义拦截器    -->
        <action dtiUpload="true" name="create" uicomponent="CREATE_DOC" ajax="row">
            <command onClick="validateCreateLocation(event)" method="execute"
                     class="ext.ziang.doc.ExtCreateDocFormProcessor" windowType="popup"/>
            <includeFilter name="projectM4D"/>
            <includeFilter name="showNewActionPreValidation"/>
        </action>
    </objecttype>

    <objecttype resourceBundle="com.ptc.windchill.principal.user.userActionsRB" name="user" class="wt.org.WTUser" >
        <action name="create" ajax="row" >
            <command method="execute"
                     class="ext.ziang.user.process.ExtCreateUserFormProcessor"
                     windowType="popup"/>
        </action>
    </objecttype>

    <objecttype resourceBundle="com.ptc.windchill.principal.user.userActionsRB" name="user" class="wt.org.WTUser" >
        <action name="updateUser" ajax="row" >
            <command method="execute"
                     class="ext.ziang.user.process.ExtEditUserFormProcessor"
                     windowType="popup"/>
            <includeFilter name="updateActionValidator"/>
            <includeFilter name="disconnectedPrincipalActionsFilter"/>
            <includeFilter name="disabledPrincipalActionsFilter"/>
        </action>
    </objecttype>

    <objecttype name="extCommonAction"
                resourceBundle="ext.ziang.resource.CommonActionResource">
        <!--一键复里面制BOM-->
        <action name="onceCopyBom">
            <command method="execute"
                     class="ext.trinasolar.eccb.process.SelectOriginBomProcessor"
                     url="/netmarkets/jsp/ext/ziang/change/copyBom/onceCopyBomOrigin.jsp"
                     windowType="popup"/>
        </action>

        <!--选择源对象-->
        <action name="selectAffectionBomStep" id="selectAffectionBomStep">
            <command url="/netmarkets/jsp/ext/ziang/change/copyBom/selectAffectionBomStep.jsp"
                     windowType="wizard_step"/>
        </action>

        <!--选择需要被复制的对象-->
        <action name="copyBomStructStep" id="copyBomStructStep">
            <command windowType="wizard_step"
                     url="/netmarkets/jsp/ext/ziang/change/copyBom/copyBomStructStep.jsp"/>
        </action>
    </objecttype>
</listofactions>
